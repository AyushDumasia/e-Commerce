{
    "routes": [
        {
            "src": "/",
            "dest": "/"
        },
        {
            "src": "/api/(.*)",
            "dest": "/"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://e-commerce-zlme.onrender.com/api/$1"
        }
    ]
}
